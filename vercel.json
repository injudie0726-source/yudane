{
    "version": 2,
    "name": "yudane",
    "public": true,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=0, must-revalidate"
            }
        }
    ]
}